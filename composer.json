{
  "name": "bendeckdavid/graphql-client",
  "description": "Graphql Client for Laravel",
  "version": "2.0.0",
  "type": "library",
  "license": "MIT",
  "author": {
    "name": "David Gutierrez",
    "github": "BendeckDavid",
    "email": "davidgbendeck@gmail.co"
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.48|^11.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.35",
    "orchestra/testbench": "^9.4",
    "pestphp/pest-plugin-laravel": "^2.4"
  },
  "keywords": [
    "Laravel",
    "Graphql",
    "Graphql Client",
    "BendeckDavid"
  ],
  "autoload": {
    "psr-4": {
      "BendeckDavid\\GraphqlClient\\": "src/",
      "BendeckDavid\\GraphqlClient\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel":{
      "providers": [
        "BendeckDavid\\GraphqlClient\\GraphqlClientServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
